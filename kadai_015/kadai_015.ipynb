      "source": [
        "# Humanクラスの定義\n",
        "class Human:\n",
        "    def __init__(self, name, age):\n",
        "        self.name = name  # 名前の属性\n",
        "        self.age = age    # 年齢の属性\n",
        "\n",
        "    def printinfo(self):\n",
        "        print(f\"名前: {self.name}, 年齢: {self.age}\")\n",
        "\n",
        "# インスタンスの作成と変数への代入\n",
        "person = Human(\"康司\", 25)\n",
        "\n",
        "# メソッドの使用\n",
        "person.printinfo()"
      ],
